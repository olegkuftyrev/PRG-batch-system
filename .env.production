# Production Environment Variables for Droplet Deployment
# Copy this to .env and fill in actual values

# Application
NODE_ENV=production
APP_KEY=your-secure-random-32-char-key-here

# Database (Local Postgres)
POSTGRES_USER=prg
POSTGRES_PASSWORD=your-secure-db-password-here
POSTGRES_DB=prg_batch
POSTGRES_PORT=5432

# API Service
API_PORT=3333

# Web Frontend
WEB_PORT=8080
